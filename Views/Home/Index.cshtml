                                {
                                    foreach (var item in Model)
                                    {
                                        if (!item.jaId.Contains("JA"))
                                        {
                                            <tr style="cursor: pointer;"><td>@item.jaId</td><td>@item.joId</td><td style="width: 320px;">@item.jaNumber</td></tr>
                                        }

                                    }
                                }
                            }
